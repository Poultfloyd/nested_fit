    # Check x0 value
    line 12 split[1] as float match range(453, 456)

    # Check amp value
    line 13 split[1] as float match range(284, 316)

    # Check sigma value
    line 14 split[1] as float match range(24, 28)
}

from nf_output_points.paramnames expect
{
    # Check bg name order
    line 1 match "bg"

    # Check x0 name order
    line 2 match "x0"

    # Check amp name order
    line 3 match "amp"

    # Check sigma name order
    line 4 match "sigma"
}
